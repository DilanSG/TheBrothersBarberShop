{
  "name": "the-brothers-barbershop-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "Backend para The Brothers Barber Shop",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon --exec \"dotenv -e .env -- node\" src/index.js",
    "start": "NODE_ENV=production node src/index.js",
    "start:prod": "NODE_ENV=production node src/index.js",
    "seed:master": "dotenv -e .env -- node scripts/master-population.js",
    "seed:inventory": "dotenv -e .env -- node scripts/seedInventoryProducts.js",
    "init:fundador": "dotenv -e .env -- node scripts/inicializar-socio-fundador.js",
    "fundador:rapido": "dotenv -e .env -- node scripts/fundador-rapido.js",
    "check:data": "dotenv -e .env -- node scripts/check-data.js",
    "clean:transactions": "dotenv -e .env -- node scripts/clean-transactions.js",
    "diagnose:expenses": "dotenv -e .env -- node scripts/diagnoseRecurringExpenses.js",
    "migrate:expenses": "dotenv -e .env -- node scripts/migrateRecurringExpenses.js",
    "analyze:data": "dotenv -e .env -- node scripts/1-analyze-data.js",
    "create:realistic-data": "dotenv -e .env -- node scripts/2-create-realistic-data.js",
    "validate:reports": "dotenv -e .env -- node scripts/3-validate-reports.js",
    "test": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles",
    "test:watch": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:unit": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js tests/unit",
    "test:integration": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js tests/integration",
    "lint": "eslint src --ext .js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mongodb-js/saslprep": "^1.3.0",
    "@sentry/node": "10.19.0",
    "@sentry/profiling-node": "10.19.0",
    "bcryptjs": "^3.0.2",
    "browserslist": "4.26.3",
    "caniuse-lite": "1.0.30001750",
    "cloudinary": "^2.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "exceljs": "4.4.0",
    "express": "^4.19.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.1",
    "express-validator": "7.2.1",
    "helmet": "^7.2.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "0.6.0",
    "mongoose": "^8.5.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "node-cache": "^5.1.2",
    "node-cron": "4.2.1",
    "nodemailer": "7.0.9",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xss-clean": "^0.1.4",
    "yamljs": "^0.3.0"
  },
  "overrides": {
    "validator": "13.15.15"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "cross-env": "^10.0.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.37.0",
    "glob": "11.0.3",
    "jest": "^29.7.0",
    "supertest": "6.3.4"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=8.0.0"
  }
}
